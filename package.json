{
  "name": "pet-finder",
  "version": "1.0.0",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": "14.15.4",
    "yarn": "1.22.10"
  },
  "browserModern": "dist/index.html",
  "targets": {
    "browserModern": {
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
    }
  },
  "scripts": {
    "dev:be": "nodemon --watch ./be-src/index.ts --exec ts-node ./be-src/dev.ts",
    "build:be": "tsc",
    "dev:fe": "parcel serve ./fe-src/index.html",
    "build:fe": "parcel build ./fe-src/index.html",
    "build": "yarn build:fe && yarn build:be",
    "start": "node ./be-dist/index.js",
    "dev": "yarn build:fe && yarn dev:be"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/node": "17.0.5",
    "@vaadin/router": "^1.7.4",
    "algoliasearch": "^4.13.1",
    "cloudinary": "^1.30.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^2.9.2",
    "nodemon": "^2.0.16",
    "parcel": "^2.6.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.2",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10"
  }
}
